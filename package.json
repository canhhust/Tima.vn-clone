    "debug": "~4.1.1",
    "ejs": "~2.6.1",
    "express": "~4.17.0",
    "express-form-data": "^2.0.8",
    "gravatar": "^1.8.0",
    "http-errors": "~1.7.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.9",
    "morgan": "~1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
